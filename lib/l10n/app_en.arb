{
    "helloWorld": "Hello World!",
    "@helloWorld": {
        "description": "The conventional newborn programmer greeting"
    },
    "title": "Flutter Localizations Tutorial",
    "@title": {
        "description": "The application title"
    },
    "hello": "Hello, {userName}",
    "@hello": {
        "description": "A message with a single parameter",
        "placeholders": {
            "userName": {
                "type": "String",
                "example": "Cid"
            }
        }
    },
    "counterSentence": "You have clicked the button",
    "@counterSentence": {
        "description": "A caption for the click counter used in conjunction with nClicks"
    },
    "nClicks": "{count, plural, =0{zero times} =1{one time} other{{count} times}}",
    "@nClicks": {
    "description": "A plural message",
    "placeholders": {
        "count": {
        "type": "num",
        "format": "compact"
        }
    }
    },
    "genders": "{gender, select, male{male} female{female} other{other}}",
    "@genders": {
    "description": "Gender options",
    "placeholders": {
        "gender": {
        "type": "String"
        }
    }
    },
    "pronoun": "{gender, select, male{he/him} female{she/her} other{they/them}}",
    "@pronoun": {
    "description": "A gendered message",
    "placeholders": {
        "gender": {
        "type": "String"
        }
    }
    },
    "escapedCharacter": "'''{{'Hello World!'}}'''",
    "@escapedCharacter": {
        "description": "Renders with four curly braces"
    }
}